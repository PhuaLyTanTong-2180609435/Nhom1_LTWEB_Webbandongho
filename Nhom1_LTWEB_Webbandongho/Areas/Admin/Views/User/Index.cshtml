@model IEnumerable<Nhom1_LTWEB_Webbandongho.Models.ApplicationUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>User</h2>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>FullName</th>
            <th>UserName</th>
            <th>age</th>
            <th>gmail</th>
            <th>SD</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FullName</td>
                <td>@user.UserName</td>
                <td>@user.Age </td>
                <td>@user.Email</td>
                <td>
                    <a asp-action="Display" asp-route-id="@user.Id">View</a> |
                    <a asp-action="Update" asp-route-id="@user.Id">Edit</a> |
                </td>
            </tr>
        }
    </tbody>
</table>